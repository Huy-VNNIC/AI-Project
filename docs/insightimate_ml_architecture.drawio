        <!-- Augmentation -->
        <mxCell id="augment-techniques" value="&lt;b&gt;Data Augmentation&lt;/b&gt;&lt;br&gt;&lt;br&gt;Techniques:&lt;br&gt;• Flip: Horizontal, Vertical&lt;br&gt;• Crop: 0-5% zoom&lt;br&gt;• Rotation: -10° to +10°&lt;br&gt;• Brightness: -5% to +5%&lt;br&gt;• Blur: up to 1px&lt;br&gt;• Noise: up to 0.1%&lt;br&gt;&lt;br&gt;Purpose:&lt;br&gt;• Balance minority classes&lt;br&gt;• Increase diversity&lt;br&gt;• Reduce overfitting" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f8cecc;strokeColor=#b85450;align=left;verticalAlign=top;" vertex="1" parent="1">
          <mxGeometry x="460" y="680" width="280" height="160" as="geometry" />
        </mxCell>
        
        <!-- Data Balancing -->
        <mxCell id="balance-data" value="&lt;b&gt;Class Balancing&lt;/b&gt;&lt;br&gt;&lt;br&gt;Problem:&lt;br&gt;• Imbalanced schemas&lt;br&gt;• LOC: 947 samples&lt;br&gt;• FP: 24 samples&lt;br&gt;• UCP: 71 samples&lt;br&gt;&lt;br&gt;Solution:&lt;br&gt;• Augment minority (FP/UCP)&lt;br&gt;• Stratified sampling&lt;br&gt;• Weighted loss functions&lt;br&gt;&lt;br&gt;Result: Better generalization" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f8cecc;strokeColor=#b85450;align=left;verticalAlign=top;" vertex="1" parent="1">
          <mxGeometry x="780" y="680" width="280" height="160" as="geometry" />
        </mxCell>
        
        <!-- LAYER 4: DATA SPLITTING -->
        <mxCell id="split-layer-title" value="&lt;b&gt;4. DATA SPLITTING &amp; PARTITIONING&lt;/b&gt;" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#fad7ac;strokeColor=#b46504;fontSize=16;fontStyle=1" vertex="1" parent="1">
          <mxGeometry x="50" y="880" width="2100" height="45" as="geometry" />
        </mxCell>
        
        <!-- Splitting Strategy -->
        <mxCell id="split-strategy" value="&lt;b&gt;Splitting Strategy&lt;/b&gt;&lt;br&gt;&lt;br&gt;Train/Test Split:&lt;br&gt;• 80% Training&lt;br&gt;• 20% Testing&lt;br&gt;&lt;br&gt;Stratification:&lt;br&gt;• 5 size quantile bins&lt;br&gt;• Preserve scale distribution&lt;br&gt;&lt;br&gt;Cross-Validation:&lt;br&gt;• 5-Fold CV on training set&lt;br&gt;• Shuffled splits&lt;br&gt;&lt;br&gt;Random Seeds:&lt;br&gt;• 10 different seeds (42-51)&lt;br&gt;• Report mean ± std" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;align=left;verticalAlign=top;" vertex="1" parent="1">
          <mxGeometry x="100" y="950" width="320" height="180" as="geometry" />
        </mxCell>
        
        <!-- Data Transformation -->
        <mxCell id="transform-log" value="&lt;b&gt;Log Transformation&lt;/b&gt;&lt;br&gt;&lt;br&gt;Apply to:&lt;br&gt;• Size (KLOC/FP/UCP)&lt;br&gt;• Effort (Person-Months)&lt;br&gt;&lt;br&gt;Formula:&lt;br&gt;log_size = log(Size)&lt;br&gt;log_effort = log(Effort)&lt;br&gt;&lt;br&gt;Benefits:&lt;br&gt;• Normalize skewed distributions&lt;br&gt;• Improve correlation&lt;br&gt;• Linear in log-space&lt;br&gt;• Better model fit" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;align=left;verticalAlign=top;" vertex="1" parent="1">
          <mxGeometry x="460" y="950" width="280" height="180" as="geometry" />
        </mxCell>
        
        <!-- Feature Scaling -->
        <mxCell id="scale-features" value="&lt;b&gt;Feature Standardization&lt;/b&gt;&lt;br&gt;&lt;br&gt;For Linear Models:&lt;br&gt;StandardScaler:&lt;br&gt;z = (x - μ) / σ&lt;br&gt;&lt;br&gt;For Tree Models:&lt;br&gt;No scaling needed&lt;br&gt;(scale-invariant)&lt;br&gt;&lt;br&gt;Fit on training only:&lt;br&gt;• Calculate μ, σ on train&lt;br&gt;• Apply to test&lt;br&gt;• Prevent data leakage" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;align=left;verticalAlign=top;" vertex="1" parent="1">
          <mxGeometry x="780" y="950" width="280" height="180" as="geometry" />
        </mxCell>
        
        <!-- Final Dataset -->
        <mxCell id="final-dataset" value="&lt;b&gt;FINAL DATASET STATS&lt;/b&gt;&lt;br&gt;&lt;br&gt;LOC Schema:&lt;br&gt;• Train: 757 samples (80%)&lt;br&gt;• Test: 190 samples (20%)&lt;br&gt;&lt;br&gt;FP Schema:&lt;br&gt;• Train: 19 samples (80%)&lt;br&gt;• Test: 5 samples (20%)&lt;br&gt;&lt;br&gt;UCP Schema:&lt;br&gt;• Train: 57 samples (80%)&lt;br&gt;• Test: 14 samples (20%)&lt;br&gt;&lt;br&gt;All datasets:&lt;br&gt;✓ Cleaned&lt;br&gt;✓ Harmonized&lt;br&gt;✓ Balanced&lt;br&gt;✓ Ready for training" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;align=left;verticalAlign=top;fontStyle=1" vertex="1" parent="1">
          <mxGeometry x="1100" y="950" width="320" height="180" as="geometry" />
        </mxCell>
        
        <!-- LAYER 5: MODEL TRAINING -->
        <mxCell id="model-layer-title" value="&lt;b&gt;5. MACHINE LEARNING MODEL TRAINING&lt;/b&gt;" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f5f5f5;strokeColor=#666666;fontSize=16;fontStyle=1" vertex="1" parent="1">
          <mxGeometry x="50" y="1170" width="2100" height="45" as="geometry" />
        </mxCell>
        
        <!-- COCOMO II Baseline -->
        <mxCell id="model-cocomo" value="&lt;b&gt;COCOMO II&lt;/b&gt;&lt;br&gt;(Baseline)&lt;br&gt;&lt;br&gt;Type: Parametric&lt;br&gt;&lt;br&gt;Formula:&lt;br&gt;Effort = A × Size^E × Π(EM_i)&lt;br&gt;&lt;br&gt;Constants:&lt;br&gt;A = 2.94 (organic)&lt;br&gt;E = 1.0-1.2&lt;br&gt;&lt;br&gt;17 Cost Drivers&lt;br&gt;5 Scale Factors&lt;br&gt;&lt;br&gt;Performance:&lt;br&gt;MMRE: 2.79&lt;br&gt;PRED(25): 0.012&lt;br&gt;MAE: 45.03 PM&lt;br&gt;RMSE: 53.70 PM" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#e1d5e7;strokeColor=#9673a6;align=left;verticalAlign=top;" vertex="1" parent="1">
          <mxGeometry x="100" y="1240" width="220" height="260" as="geometry" />
        </mxCell>
        
        <!-- Linear Regression -->
        <mxCell id="model-lr" value="&lt;b&gt;Linear Regression&lt;/b&gt;&lt;br&gt;&lt;br&gt;Type: ML Baseline&lt;br&gt;Library: sklearn&lt;br&gt;&lt;br&gt;Variants:&lt;br&gt;1. Standard LR&lt;br&gt;2. Log-Log LR&lt;br&gt;&lt;br&gt;Hyperparameters:&lt;br&gt;• fit_intercept: True&lt;br&gt;• normalize: False&lt;br&gt;&lt;br&gt;Training:&lt;br&gt;• Closed-form solution&lt;br&gt;• Fast (&lt;1 sec)&lt;br&gt;&lt;br&gt;Performance:&lt;br&gt;MMRE: 4.50&lt;br&gt;PRED(25): 0.000&lt;br&gt;MAE: 107.54 PM&lt;br&gt;RMSE: 280.27 PM&lt;br&gt;&lt;br&gt;Issue: High bias&lt;br&gt;Unstable predictions" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#b1ddf0;strokeColor=#10739e;align=left;verticalAlign=top;" vertex="1" parent="1">
          <mxGeometry x="360" y="1240" width="220" height="260" as="geometry" />
        </mxCell>
        
        <!-- Decision Tree -->
        <mxCell id="model-dt" value="&lt;b&gt;Decision Tree&lt;/b&gt;&lt;br&gt;&lt;br&gt;Type: Non-linear ML&lt;br&gt;Library: sklearn&lt;br&gt;&lt;br&gt;Hyperparameters:&lt;br&gt;• max_depth: 3-15&lt;br&gt;• min_samples_leaf: 2-10&lt;br&gt;• min_samples_split: 2-20&lt;br&gt;• criterion: squared_error&lt;br&gt;&lt;br&gt;Tuning: GridSearchCV&lt;br&gt;• 5-Fold CV&lt;br&gt;• Metric: RMSE&lt;br&gt;&lt;br&gt;Training Time: ~5 sec&lt;br&gt;&lt;br&gt;Performance:&lt;br&gt;MMRE: 1.37&lt;br&gt;PRED(25): 0.173&lt;br&gt;MAE: 18.63 PM&lt;br&gt;RMSE: 23.62 PM&lt;br&gt;&lt;br&gt;Pros: Interpretable&lt;br&gt;Cons: Overfits easily" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#b1ddf0;strokeColor=#10739e;align=left;verticalAlign=top;" vertex="1" parent="1">
          <mxGeometry x="620" y="1240" width="240" height="260" as="geometry" />
        </mxCell>
        
        <!-- Random Forest (BEST) -->
        <mxCell id="model-rf" value="&lt;b&gt;Random Forest ⭐&lt;/b&gt;&lt;br&gt;&lt;b&gt;BEST MODEL&lt;/b&gt;&lt;br&gt;&lt;br&gt;Type: Ensemble (Bagging)&lt;br&gt;Library: sklearn&lt;br&gt;&lt;br&gt;Hyperparameters:&lt;br&gt;• n_estimators: 50-300&lt;br&gt;• max_features: sqrt, log2&lt;br&gt;• max_depth: 5-20&lt;br&gt;• min_samples_leaf: 2-5&lt;br&gt;• bootstrap: True&lt;br&gt;&lt;br&gt;Tuning: GridSearchCV&lt;br&gt;• 5-Fold CV&lt;br&gt;• Metric: RMSE&lt;br&gt;&lt;br&gt;Training Time: ~30 sec&lt;br&gt;&lt;br&gt;Performance:&lt;br&gt;MMRE: 0.65 ✓&lt;br&gt;PRED(25): 0.395 ✓&lt;br&gt;MAE: 12.66 PM ✓&lt;br&gt;RMSE: 20.01 PM ✓&lt;br&gt;&lt;br&gt;Improvement vs COCOMO:&lt;br&gt;• MMRE: -76.8%&lt;br&gt;• PRED(25): +32x" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;align=left;verticalAlign=top;fontStyle=1" vertex="1" parent="1">
          <mxGeometry x="900" y="1240" width="260" height="260" as="geometry" />
        </mxCell>
        
        <!-- Gradient Boosting -->
        <mxCell id="model-gb" value="&lt;b&gt;Gradient Boosting&lt;/b&gt;&lt;br&gt;&lt;br&gt;Type: Ensemble (Boosting)&lt;br&gt;Library: sklearn&lt;br&gt;&lt;br&gt;Hyperparameters:&lt;br&gt;• learning_rate: 0.01-0.3&lt;br&gt;• n_estimators: 50-500&lt;br&gt;• max_depth: 3-10&lt;br&gt;• subsample: 0.6-1.0&lt;br&gt;• min_samples_leaf: 2-5&lt;br&gt;&lt;br&gt;Tuning: GridSearchCV&lt;br&gt;• Early stopping&lt;br&gt;• Validation split: 10%&lt;br&gt;&lt;br&gt;Training Time: ~45 sec&lt;br&gt;&lt;br&gt;Performance:&lt;br&gt;MMRE: 1.10&lt;br&gt;PRED(25): 0.198&lt;br&gt;MAE: 16.16 PM&lt;br&gt;RMSE: 21.09 PM&lt;br&gt;&lt;br&gt;2nd Best Model" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#b1ddf0;strokeColor=#10739e;align=left;verticalAlign=top;" vertex="1" parent="1">
          <mxGeometry x="1200" y="1240" width="240" height="260" as="geometry" />
        </mxCell>
        
        <!-- LAYER 6: EVALUATION -->
        <mxCell id="eval-layer-title" value="&lt;b&gt;6. MODEL EVALUATION &amp; VALIDATION&lt;/b&gt;" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#fad7ac;strokeColor=#b46504;fontSize=16;fontStyle=1" vertex="1" parent="1">
          <mxGeometry x="50" y="1540" width="2100" height="45" as="geometry" />
        </mxCell>
        
        <!-- Evaluation Metrics -->
        <mxCell id="eval-metrics" value="&lt;b&gt;EVALUATION METRICS&lt;/b&gt;&lt;br&gt;&lt;br&gt;1. MMRE (Mean Magnitude Relative Error):&lt;br&gt;   MMRE = (1/n) × Σ|y_i - ŷ_i| / y_i&lt;br&gt;&lt;br&gt;2. PRED(25) (Prediction at 25%):&lt;br&gt;   PRED(25) = count(|y_i - ŷ_i|/y_i ≤ 0.25) / n&lt;br&gt;&lt;br&gt;3. MAE (Mean Absolute Error):&lt;br&gt;   MAE = (1/n) × Σ|y_i - ŷ_i|&lt;br&gt;&lt;br&gt;4. RMSE (Root Mean Squared Error):&lt;br&gt;   RMSE = √[(1/n) × Σ(y_i - ŷ_i)²]&lt;br&gt;&lt;br&gt;5. R² (Coefficient of Determination):&lt;br&gt;   R² = 1 - [Σ(y_i - ŷ_i)² / Σ(y_i - ȳ)²]&lt;br&gt;&lt;br&gt;Computed on:&lt;br&gt;• Test set (20%)&lt;br&gt;• 10 random seeds&lt;br&gt;• Report: mean ± std" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#fff2cc;strokeColor=#d6b656;align=left;verticalAlign=top;" vertex="1" parent="1">
          <mxGeometry x="100" y="1610" width="420" height="240" as="geometry" />
        </mxCell>
        
        <!-- Statistical Testing -->
        <mxCell id="eval-stats" value="&lt;b&gt;STATISTICAL TESTING&lt;/b&gt;&lt;br&gt;&lt;br&gt;Wilcoxon Signed-Rank Test:&lt;br&gt;• Non-parametric&lt;br&gt;• Paired samples&lt;br&gt;• H0: No difference&lt;br&gt;• α = 0.05&lt;br&gt;&lt;br&gt;Holm-Bonferroni Correction:&lt;br&gt;• Multiple comparisons&lt;br&gt;• Control family-wise error&lt;br&gt;&lt;br&gt;Cliff's Delta (Effect Size):&lt;br&gt;• δ &lt; 0.147: Negligible&lt;br&gt;• 0.147-0.33: Small&lt;br&gt;• 0.33-0.474: Medium&lt;br&gt;• &gt; 0.474: Large&lt;br&gt;&lt;br&gt;Results:&lt;br&gt;✓ RF significantly better&lt;br&gt;✓ p &lt; 0.001&lt;br&gt;✓ Large effect size" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#fff2cc;strokeColor=#d6b656;align=left;verticalAlign=top;" vertex="1" parent="1">
          <mxGeometry x="560" y="1610" width="320" height="240" as="geometry" />
